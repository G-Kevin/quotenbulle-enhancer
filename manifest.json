{
  "name": "Quotenbulle.de Orbit linkchanger",
  "description": "Extension to change orbit links at Quotenbulle.de",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "favicon.ico"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.quotenbulle.de/tools/rechner/*"
      ],
      "run_at": "document_end",
      "js": [
        "config.js",
        "jquery-3.6.0.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
     "https://*.quotenbulle.de/tools/rechner/*"
  ]
}